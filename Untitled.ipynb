    "s = \"camelCasingWorld\"\n",
    "s1 = [i for i in s]\n",
    "s2 =s1.copy()\n",
    "output = []\n",
    "count = 0\n",
    "for index,i in enumerate(s1):\n",
    "    if i.isupper():        \n",
    "        s2.insert(index+count,\" \")\n",
    "        count+=1        \n",
    "\"\".join(s2)"
